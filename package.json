{
  "name": "shrewd",
  "version": "0.4.1",
  "description": "A simple API which gives lunar phase information",
  "repository": {
    "type": "git",
    "url": "https://github.com/free-ignorance/shrewd"
  },
  "bugs": {
    "url": "https://github.com/free-ignorance/shrewd/issues"
  },
